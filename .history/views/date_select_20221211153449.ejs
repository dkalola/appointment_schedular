        <div class="col-2 apform"> <input  class="form-control" id="date" type="datetime-local"/></div>
        <% if (type == 1) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-primary" onclick="myFunction()">Submit</button></div>
        <%} else if (type == 2) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-success" onclick="myFunction()">Submit</button></div>

        <%} else if (type == 3) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-danger" onclick="myFunction()">Submit</button></div>

        <%} else if (type == 4) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-warning" onclick="myFunction()">Submit</button></div>

        <%} else if (type == 5) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-info" onclick="myFunction()">Submit</button></div>
        
        <%} else if (type == 6) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-light" onclick="myFunction()">Submit</button></div>
        
        <%} else if (type == 7) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-dark" onclick="myFunction()">Submit</button></div>
        <%} else if (type == 8) { %>
            <div class="col-1 apform"><button type="button" class="btn btn-link" onclick="myFunction()">Submit</button></div>
        <%} else if (color) { %>
            <div class="col-1 apform"><button type="button" class="btn" style="background-color: #<%= btnc %>;color:#<%= color %>" onclick="myFunction()">Submit</button></div>
        <% } %>
    </div>
   
