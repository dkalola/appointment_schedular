     <div class="row text-center">
        <div class="col"> <p>Select Date and Time to submit the appointment.</p></div>
    </div>

 
    <div class="row d-flex justify-content-center">
        <div class="col-2"> <input  class="form-control" type="datetime-local"/></div>
        <% if (type == 1) { %>
            <div class="col-1"><button type="button" class="btn btn-primary" onclick="myFunction()">Submit</button></div>
        <%} else if (type == 2) { %>
            <div class="col-1"><button type="button" class="btn btn-success">Submit</button></div>

        <%} else if (type == 3) { %>
            <div class="col-1"><button type="button" class="btn btn-danger">Submit</button></div>

        <%} else if (type == 4) { %>
            <div class="col-1"><button type="button" class="btn btn-warning">Submit</button></div>

        <%} else if (type == 5) { %>
            <div class="col-1"><button type="button" class="btn btn-info">Submit</button></div>
        
        <%} else if (type == 6) { %>
            <div class="col-1"><button type="button" class="btn btn-light">Submit</button></div>
        
        <%} else if (type == 7) { %>
            <div class="col-1"><button type="button" class="btn btn-dark">Submit</button></div>
        <%} else if (type == 8) { %>
            <div class="col-1"><button type="button" class="btn btn-link">Submit</button></div>
        <%} else if (color) { %>
            <div class="col-1"><button type="button" class="btn" style="background-color: #<%= btnc %>;color:#<%= color %>">Submit</button></div>
        <% } %>
    </div>
   
